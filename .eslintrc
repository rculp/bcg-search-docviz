{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "linebreak-style": 0,
    "function-paren-newline": 0,
    "comma-dangle": 0,
    "max-len": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"] // Must match NODE_PATH
      }
    }
  }
}